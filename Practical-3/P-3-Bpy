# Create three dictionaries
dict1 = {'a': 1, 'b': 2}
dict2 = {'c': 3, 'd': 4}
dict3 = {'e': 5, 'f': 6}

# Merge dictionaries into one
merged_dict = {}
merged_dict.update(dict1)
merged_dict.update(dict2)
merged_dict.update(dict3)

# Display the merged dictionary
print("Merged Dictionary:", merged_dict)

# Apply dictionary operations
# Delete an item
del merged_dict['a']

# Clear the dictionary
merged_dict.clear()

# Update a dictionary
dict1.update({'g': 7})

# Pop an item
popped_item = dict1.pop('b')

# Pop an arbitrary item
pop_item = dict2.popitem()

# Get a value
value = dict3.get('f')

# Get keys and values
keys = dict1.keys()
values = dict2.values()

# Display the dictionary after operations
print("Dictionary after operations:", dict1)
print("Popped item:", popped_item)
print("Popped item (popitem()):", pop_item)
print("Value for key 'f':", value)
print("Keys:", keys)
print("Values:", values)
